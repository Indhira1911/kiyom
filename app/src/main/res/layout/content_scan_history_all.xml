<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_scan_history_all"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.v7ench.kiyo.Scan_history_all"
    tools:showIn="@layout/activity_scan_history_all">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:background="#26306f"
        android:layout_alignParentTop="true"
        android:id="@+id/home_mainima"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="170dp"
            android:layout_height="170dp"
            app:srcCompat="@drawable/scan_history"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/imageView3" />
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_below="@id/home_mainima"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"
                app:cardCornerRadius="5dp"
                app:cardPreventCornerOverlap="true"
                app:cardElevation="3dp"
                android:id="@+id/first_main_card"
                android:layout_height="wrap_content"

                android:layout_marginTop="25dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                    <ImageView
                        android:padding="30dp"
                        android:id="@+id/image_maintst"
                        android:layout_width="60dp"
                        android:layout_gravity="center_vertical"
                        android:layout_height="60dp"
                        android:background="@drawable/tst_main"
                        />
                    <LinearLayout
                        android:layout_width="0dp"
                        android:padding="2dp"
                        android:layout_weight="3"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#2d2d2d"
                            android:textSize="20sp"
                            android:paddingTop="8dp"
                            android:paddingRight="8dp"
                            android:paddingLeft="8dp"
                            android:gravity="center_vertical"
                            android:text="@string/tst_test"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:maxLines="2"
                            android:layout_gravity="center_vertical"
                            android:padding="4dp"
                            android:text="@string/compare_pre_test_and_post_test_results_to_get_accurate_test_results"
                            android:textColor="#2d2d2d"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_gravity="center"
                                android:background="@drawable/scan_tube_test"
                                android:id="@+id/imageView21" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:id="@+id/testnum_tst"
                                android:textSize="12sp"

                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_gravity="center"
                                android:background="@drawable/scan_safe"
                                android:id="@+id/image_safe" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="12sp"
                                android:id="@+id/testnum_tst_safe"

                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"



                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_gravity="center"
                                android:background="@drawable/scan_s1_wrong"
                                android:id="@+id/image_unsafe" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="12sp"
                                android:id="@+id/testnum_tst_unsafe"

                                />
                        </LinearLayout>
                            </LinearLayout>
                    </LinearLayout>
                    <ImageButton
                        android:id="@+id/tstma"
                        android:background="?android:selectableItemBackground"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        app:srcCompat="@drawable/ic_chevron_right_black_24dp"
                        />

                </LinearLayout>
