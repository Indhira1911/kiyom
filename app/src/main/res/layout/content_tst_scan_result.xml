<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_tst_scan_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.v7ench.kiyo.TstScanResult"
    tools:showIn="@layout/activity_tst_scan_result">


    <TextView
        android:text="Dr.kumar vel "

        android:layout_width="wrap_content"
        android:textColor="#2d2d2d"
        android:textSize="20sp"
        android:layout_height="wrap_content"
        android:id="@+id/tstdocname"
        android:layout_marginTop="23dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
            android:text="Contents"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
        android:layout_marginTop="55dp"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_below="@+id/tstdocname"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18sp" />

    <TextView
        android:text="Pre Test Color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_marginTop="26dp"
        android:id="@+id/textView10"
        android:textSize="18sp" />

    <TextView
        android:text="Post Test Color"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_marginTop="23dp"
        android:id="@+id/textView11"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView10"
        android:layout_toRightOf="@+id/textView11"
        android:layout_toEndOf="@+id/textView11"
        android:layout_marginLeft="69dp"
        android:layout_marginStart="69dp"
        android:id="@+id/content_text"
        android:textSize="18sp"
        android:textColor="#2d2d2d" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView11"
        android:layout_alignLeft="@+id/content_text"
        android:layout_alignStart="@+id/content_text"
        android:id="@+id/pretest_color"
        android:textColor="#2d2d2d"
        android:textSize="18sp" />

    <TextView
        android:text="postcolor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignLeft="@+id/pretest_color"
        android:layout_alignStart="@+id/pretest_color"
        android:id="@+id/post_test_color"
        android:textSize="18sp"
        android:textColor="#2d2d2d" />

</RelativeLayout>
