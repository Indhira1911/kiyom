<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_tst_scan_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.v7ench.kiyo.TstScanResult"
    tools:showIn="@layout/activity_tst_scan_result">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:id="@+id/tst_cont"
    android:gravity="center"
    android:orientation="vertical"
    android:background="#26306f">
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/test_alpha_safe_new"
        android:id="@+id/imageView7"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TST Test Report"
        android:textSize="20sp"
        android:textColor="#fff"
        android:id="@+id/textViewabc4"
      />


</LinearLayout>

    <RelativeLayout
        android:layout_margin="10dp"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:id="@+id/conte_tst"

        android:layout_height="wrap_content"
        android:background="@drawable/letsdraw2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="46dp">

        <TextView
            android:text="PRE TEST COLOR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView9"
            android:layout_alignLeft="@+id/textView9"
            android:layout_alignStart="@+id/textView9"
            android:layout_marginTop="23dp"
            android:id="@+id/textView10"
            android:textSize="14sp" />

        <TextView
            android:text="POST TEST COLOR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView10"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:layout_marginTop="23dp"
            android:id="@+id/textView11"
            android:textSize="14sp" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView10"
            android:layout_toRightOf="@+id/textView11"
            android:layout_toEndOf="@+id/textView11"
            android:layout_marginLeft="69dp"
            android:layout_marginStart="69dp"
            android:id="@+id/content_textv"
            android:textSize="12sp"
            android:textColor="#2d2d2d" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView11"
            android:layout_alignLeft="@+id/content_textv"
            android:layout_alignStart="@+id/content_textv"
            android:id="@+id/pretest_date"
            android:textColor="#2d2d2d"
            android:textSize="12sp" />

        <TextView
            android:text="postcolor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView11"
            android:layout_alignLeft="@+id/pretest_date"
            android:layout_alignStart="@+id/pretest_date"
            android:id="@+id/post_test_colorv"
            android:textSize="12sp"
            android:textColor="#2d2d2d" />

        <TextView
            android:text="TEST RESULT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:textSize="14sp"
            android:layout_marginTop="23dp"
            android:layout_below="@+id/textView11"
            android:layout_alignLeft="@+id/textView11"
            android:layout_alignStart="@+id/textView11" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView12"
            android:layout_alignLeft="@+id/post_test_colorv"
            android:textSize="12sp"
            android:textColor="#2d2d2d"
            android:layout_alignStart="@+id/post_test_colorv"
            android:id="@+id/test_result"
            android:textStyle="normal|bold" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/normal_qr"
            android:id="@+id/imageView16"
          android:layout_below="@id/textView12"
            android:layout_marginTop="23dp"
            android:layout_alignLeft="@+id/textView12"
            android:layout_alignStart="@+id/textView12" />

        <TextView
            android:text="KISTAAAADD13112016"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/qr_scanned"
            android:textSize="14sp"
            android:textColor="#2d2d2d"
            android:layout_marginTop="12dp"
            android:layout_marginLeft="10dp"
            android:layout_alignTop="@+id/imageView16"
            android:layout_toRightOf="@+id/imageView16"
            android:layout_toEndOf="@+id/imageView16" />

        <TextView
                android:text="CONTENTS"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView9"
            android:layout_marginTop="19dp"
            android:layout_marginLeft="23dp"
            android:layout_marginStart="23dp"
            android:textSize="14sp"
            android:layout_below="@+id/biodocnamev"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:text="Dr.kumar vel "
            android:layout_width="wrap_content"
            android:textColor="#2d2d2d"
            android:textSize="20sp"
            android:layout_height="wrap_content"
            android:id="@+id/biodocnamev"
            android:layout_marginTop="14dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>
</RelativeLayout>
